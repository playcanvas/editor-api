{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "./src/index.ts"
    ],
    "exclude": [
        "**/node_modules/**"
    ],
    "excludeNotDocumented": true,
    "favicon": "utils/typedoc/favicon.ico",
    "hostedBaseUrl": "https://api.playcanvas.com/editor/",
    "includeVersion": true,
    "name": "Editor API Reference",
    "navigationLinks": {
        "Developer Site": "https://developer.playcanvas.com/",
        "Blog": "https://blog.playcanvas.com/",
        "Discord": "https://discord.gg/RSaMRzg",
        "Forum": "https://forum.playcanvas.com/",
        "GitHub": "https://github.com/playcanvas/editor-api"
    },
    "sidebarLinks": {
        "Home": "/"
    },
    "plugin": [
        "typedoc-plugin-mdn-links"
    ],
    "readme": "none",
    "searchGroupBoosts": {
        "Classes": 2
    }
}
